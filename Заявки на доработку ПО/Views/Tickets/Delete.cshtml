@model Заявки_на_доработку_ПО.Classes.Ticket

@{
    ViewData["Title"] = "Удалить";
}

<h2>Удалить</h2>

<h3>Вы уверены, что хотите удалить это?</h3>
<div>
    <h4>Заявка</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Название)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Название)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Описание)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Описание)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ДатаОкончанияРазработки)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ДатаОкончанияРазработки)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-action="Index">Вернуться к списку</a>
    </form>
</div>
